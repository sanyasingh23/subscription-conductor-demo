[
  {
    "name": "subscription_workflow",
    "description": "Subscription Details",
    "version": 1,
    "schemaVersion": 2,
    "inputParameters": [
      "name"
    ],
    "tasks": [
      {
        "name": "subscription",
        "taskReferenceName": "subscription_info",
        "inputParameters": {
          "name": "${workflow.input.customerName}",
          "country": "${workflow.input.country}",
          "eaId": "${workflow.input.eaId}",
          "region": "${workflow.input.region}",
          "status": "${workflow.input.status}"
        },
        "type": "SIMPLE"
      }
    ]
  },
  {
    "name": "license_workflow",
    "description": "License Details",
    "version": 1,
    "schemaVersion": 2,
    "inputParameters": [
      "name"
    ],
    "tasks": [
      {
        "name": "license_1",
        "taskReferenceName": "license_1_info",
        "inputParameters": {
          "productType": "${workflow.input.productType}",
          "quantity": "${workflow.input.quantity}",
          "startDate": "${workflow.input.startDate}",
          "endDate": "${workflow.input.endDate}",
          "classification": "${workflow.input.classification}",
          "licenseType": "${workflow.input.licenseType}",
          "billingType": "${workflow.input.billingType}",
          "status": "${workflow.input.status}"
        },
        "type": "SIMPLE"
      }
    ],
    "outputParameters": {
      "hcsSubscriptionId": "${subscription.subscription_info.input.customerName}"
    }
  }
]